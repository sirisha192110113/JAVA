class Box
{
    double width,height,depth;
    Box(double w,double h,double d)
    {
        width=w;
        height=h;
        depth=d;
    }
    double volume()
    {
        return width*height*depth;
    }
    public static void main(String args[])
    {
        Box b3=new Box(10.5,20.5,30.5);
        double res=b3.volume();
        System.out.println(res);
    }
}
