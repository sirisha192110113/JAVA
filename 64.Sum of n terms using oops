import java.util.*;
class Sum
{
    int n,i,sum=0;
    void add()
    {
        Scanner s=new Scanner(System.in);
        System.out.println("enter number of terms");
        n=s.nextInt();
        for(i=1;i<=n;i++)
        sum=sum+i;
        System.out.println("sum is"+sum);
    }
}
class addition
{
    public static void main(String args[])
    {
        Sum obj=new Sum();
        obj.add();
    }
}
