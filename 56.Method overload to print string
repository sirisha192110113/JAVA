import java.util.*;
class Over
{
    void welcome()
    {
        System.out.println("welcome to java");
    }
    void welcome(String a)
    {
        for(int i=0;i<2;i++)
        System.out.println(a);
    }
    void welcome(String  a,int b)
    {
        for(int i=1;i<4;i++)
        System.out.println("result is"+a+b);
    }
}
class overload
{
public static void main(String args[])
{
    Over obj=new Over();
    obj.welcome();
    obj.welcome("welcome to polymorphism");
    obj.welcome("welcome to overloading");
}
}
